.bonus-patient {
  padding: 20px;
  border-bottom: 1px solid #cad6de;
}
.bonus-patient__list {
  display: inline-block;
  float: left;
  vertical-align: top;
  padding-left: 40px;
  width: 50%;
}
.bonus-patient__list > ul > li {
  font-family: 'PTSansItalic', arial, sans-serif;
}
.bonus-patient__list-title {
  font-family: 'OpenSansBold', sans-serif;
  font-size: 24px;
  color: #000000;
  position: relative;
  padding: 0;
  text-transform: uppercase;
}
.bonus-patient__list-title:before {
  content: '';
  display: block;
  width: 30px;
  height: 30px;
  position: absolute;
  top: 5px;
  left: -40px;
  background: url("../img/sprite_index.png") no-repeat 0 -43px;
}
.bonus-patient__wrap .personal-info__title {
  background-color: #f2f8fb;
  padding: 0;
}
.bonus-patient__wrap .personal-info__title p {
  font-family: 'OpenSansBold', sans-serif;
  color: #000000;
  position: relative;
  display: block;
  background-color: #ddebf3;
  float: left;
  font-size: 18px;
  padding: 10px 10px 10px 20px;
  margin-right: 40px;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -o-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
}
.bonus-patient__wrap .personal-info__title p:after {
  content: '';
  display: block;
  position: absolute;
  border-left: 20px solid #ddebf3;
  border-right: 20px solid transparent;
  border-top: 26px solid transparent;
  border-bottom: 26px solid transparent;
  top: 0;
  right: -40px;
}
.bonus-patient__wrap .personal-info__title ul li {
  display: inline-block;
  vertical-align: middle;
}
.bonus-patient__wrap .personal-info__title ul li a {
  color: #115c89;
  font-size: 18px;
  display: block;
  text-decoration: underline;
  padding: 14px 35px;
}
.bonus-patient__wrap .personal-info__title ul li a:hover {
  text-decoration: none;
}
.bonus-patient__text {
  display: block;
  padding-bottom: 15px;
  position: relative;
  padding-left: 15px;
  font-family: PTSansItalic, arial, sans-serif;
}
.bonus-patient__text:before {
  content: '';
  display: block;
  width: 30px;
  height: 30px;
  position: absolute;
  top: 5px;
  left: -40px;
  background: url(../img/sprite_index.png) 0 -43px no-repeat;
}
.bonus-patient__balance {
  display: inline-block;
  float: right;
  vertical-align: top;
}
.bonus-patient__balance-title {
  font-family: 'OpenSansBold', sans-serif;
  font-size: 18px;
  color: #000000;
  padding-bottom: 10px;
  line-height: 1em;
  display: inline-block;
}
.bonus-patient__balance-score {
  font-family: 'OpenSans', sans-serif;
  font-size: 24px;
  color: #e84b3d;
  padding-bottom: 10px;
  line-height: 1em;
  display: inline-block;
}
.bonus-patient__balance-qw {
  font-family: 'OpenSans', sans-serif;
  color: #115c89;
  font-size: 14px;
  padding: 0;
}
.bonus-patient__balance > a {
  display: block;
  font-family: 'OpenSansSemiBoldItalic', sans-serif;
  font-size: 17px;
  color: #115c89;
  text-align: left;
  padding: 0;
  position: relative;
  text-decoration: underline;
}
.bonus-patient__balance > a:hover {
  text-decoration: none;
}
.bonus-patient__action {
  padding-top: 20px;
}
.bonus-patient__action-list {
  background-color: #f5f5f5;
  padding: 10px 5px 5px 5px;
  display: inline-block;
  float: left;
  vertical-align: top;
}
.bonus-patient__action-list > p {
  font-family: 'OpenSansBold', sans-serif;
  font-size: 18px;
  color: #000000;
  text-transform: uppercase;
  padding: 0 30px 5px 20px;
}
.bonus-patient__action-list > ul > li {
  background-color: #ffffff;
  border-bottom: 1px solid #f5f5f5;
  display: list-item;
  text-align: left;
  padding: 10px 20px;
}
.bonus-patient__action-list > ul > li > a {
  display: block;
  font-weight: 300;
  font-family: 'OpenSans', sans-serif;
  color: #115c89;
  text-transform: none;
  padding: 0;
}
.bonus-patient__action-list > ul > li:hover {
  background-color: #e74c3c;
}
.bonus-patient__action-list > ul > li:hover > a {
  color: #ffffff;
}
.bonus-patient__action-text {
  display: inline-block;
  float: inherit;
  vertical-align: top;
  max-width: 66%;
  padding: 0 4%;
}
.bonus-patient__action-title {
  font-family: 'OpenSans', sans-serif;
  color: #000000;
  font-size: 24px;
}
.bonus-patient__action > p {
  font-family: 'OpenSans', sans-serif;
  font-size: 14px;
  color: #6d6d6d;
}
.bonus-patient__action-img {
  display: inline-block;
  vertical-align: top;
  float: right;
  width: 185px;
}
.bonus-patient__action-img img {
  width: 100%;
}

.bonus__tovar ul {
  text-align: center;
}
.bonus__tovar ul li {
  display: inline-block;
  padding-bottom: 20px;
  margin-right: 10px;
}
.bonus__tovar-img {
  width: 210px;
