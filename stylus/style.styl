/*main start*/

.diagnostic-wrap {
  border: 0;
  margin-bottom: 40px;
  width: 100%;
}

.diagnostic__steps {
  background: url("../img/about-st.jpg") no-repeat 370px 50px;
  background-size: 67%;
}

.steplist {
  padding-top: 50px;
  margin-top: -20px;
}

.steplist__item {
  display: block;
  margin-bottom: 10px;
}

.steplist__item_inline {
  display: inline-block;
  padding-right: 25px;
  vertical-align: middle;
}

.steplist__item-step {
  display: inline-block;
  background: url("../img/spritesheet.png") no-repeat center;
  width: 74px;
  height: 73px;
  overflow: hidden;
  zoom: 0.8;
  zoom: 80%;
  -webkit-zoom: 0.8;
  -moz-transform: scale(0.8);
  -moz-transform-origin: left center;
  -ms-zoom: 0.8;
}

.steplist__item-step.step1 {
  background-position: -301px -5px;
}

.steplist__item-step.step2 {
  background-position: -385px -5px;
}

.steplist__item-step.step3 {
  background-position: -469px -5px;
}

.steplist__item_title {
  font-family: 'PTSansBold', sans-serif;
  color: #5188a9;
  font-size: 33px;
  font-weight: 700;
  padding-bottom: 0;
}

.steplist__item_text {
  white-space: normal;
  display: inline-block;
  font-family: 'OpenSans', sans-serif;
  color: #6c6c6c;
  font-size: 14px;
  font-weight: 400;
  line-height: 1.3em;
  padding-bottom: 5px;
}

.diagnostic__go {
  text-align: left;
  padding-bottom: 30px;
}

.diagnostic__go_btn {
  background-color: #00a79d;
  background-image: linear-gradient(to top, #29D5C5 0%, #00a79d 100%);
  border: 1px solid #00a79d;
  font-family: 'OpenSans', sans-serif;
  font-weight: 400;
  font-size: 16px;
  color: white;
  border-radius: 25px;
  padding: 10px 69px;
  display: inline-block;
}

.ourproject {
  text-align: center;
  border: 3px solid #ccdeeb;
}

.ourproject__title {
  display: inline-block;
  padding: 20px;
  font-family: 'OpenSans', sans-serif;
  font-size: 23px;
  font-weight: 700;
  line-height: 20px;
  color: #007b74;
  text-transform: uppercase;
}

.ourproject__list_item {
  padding: 20px 50px;
  width: 180px;
}

.ourproject__list_item-img {
  width: 64px;
  height: 64px;
  background-repeat: no-repeat;
  background-image: url(../img/spritesheet.png);
  display: inline-block;
  margin-right: 30px;
}

.ourproject__list_item-img--1 {
  background-position: -5px -5px;
}

.ourproject__list_item-img--2 {
  background-position: -79px -5px;
}

.ourproject__list_item-img--3 {
  background-position: -153px -5px;
}

.ourproject__list_item-img--4 {
  background-position: -227px -5px;
  margin-right: 20px;
}

.ourproject__list_item-link {
  padding: 5px 0;
  display: table;
  font-family: 'OpenSans', sans-serif;
  color: #007b74;
  font-size: 18px;
  font-weight: 600;
}

.ourproject__list_item-text {
  font-family: 'OpenSans', sans-serif;
  color: #6c6c6c;
  font-size: 13px;
  font-weight: 400;
  white-space: normal;
  display: table;
  line-height: 18px;
}

/*main end*/

.main_header {
  font-weight: 800;
  font-family: 'PTSansBold', sans-serif;
  text-align: center;
  padding-top: 30px;
  padding-bottom: 0;
